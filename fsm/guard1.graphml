<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
    <key id="d0" for="edge" attr.name="event" attr.type="string"/>
    <key id="d1" for="edge" attr.name="guard" attr.type="string"/>
    <key id="d2" for="node" attr.name="command" attr.type="string"/>
    <graph id="G" edgedefault="directed">
        <node id="S"/>
        <node id="A">
            <data key="d2">x=4</data>
        </node>
        <node id="B"/>
        <node id="C"/>
        <node id="D"/>
        <edge id="a" source="S" target="A">
            <data key="d0">A</data>
        </edge>
        <edge id="b" source="A" target="B">
            <data key="d0">B</data>
        </edge>
        <edge id="c" source="B" target="C">
            <data key="d0">C</data>
            <data key="d1">x&gt;5</data>
        </edge>
        <edge id="d" source="C" target="D">
            <data key="d0">D</data>
            <data key="d1">x&lt;5</data>
        </edge>
        <edge id="e" source="D" target="S">
            <data key="d0">S</data>
        </edge>
    </graph>
</graphml>
